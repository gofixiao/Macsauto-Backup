namespace Macsauto.Presentation.WinForm.PointOfSalesModule.Customer
{
    using System;

    public class MembershipTransactionDto
    {
        public MembershipTransactionDto(string id, string type, string code, DateTime date, long total, long points)
        {
            Id = id;
            Type = type;
            Code = code;
            Date = date;
            Total = total;
            Points = points;
        }

        public string Id { get; protected set; }

        public string Type { get; protected set; }

        public string Code { get; protected set; }

        public DateTime Date { get; protected set; }

        public long Total { get; protected set; }

        public long Points { get; protected set; }
    }
}